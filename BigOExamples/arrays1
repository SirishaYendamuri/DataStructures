const arr = ["array"] //single array
const large = new Array(1000).fill("sample");
function findArray(a){
  let t0 = performance.now();
  for(let i=0;i<a.length;i++){
    if(a[i] === "array")
    {
      console.log("word found");
    }
  }
  let t1 = performance.now();
  console.log("time taken for this loop is " + (t1-t0) milliseconds)
}

findArray(arr);